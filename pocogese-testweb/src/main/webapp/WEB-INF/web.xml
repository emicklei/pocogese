<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
	"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp">
	<display-name>Pocogese Test Web</display-name>

	<servlet>
		<servlet-name>PocogeseTestWeb</servlet-name>
		<display-name>PocogeseTestWeb</display-name>
		<servlet-class>com.philemonworks.pocogese.Servlet</servlet-class>
		<init-param>
			<param-name>configurator</param-name>
			<param-value>example.service.PocogeseConfigurator</param-value>
		</init-param>
		
	</servlet>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>